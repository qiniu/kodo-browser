{
  "name": "kodo-browser",
  "version": "2.0.0-alpha",
  "license": "Apache-2.0",
  "author": {
    "name": "Rong Zhou",
    "email": "zhourong@qiniu.com"
  },
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "main": "main/main-bundle.js",
  "scripts": {
    "dev": "./node_modules/.bin/webpack --env development -c ./webpack/webpack.config.js && ./node_modules/.bin/npm-run-all --parallel watch dev:run",
    "dev:run": "./node_modules/.bin/cross-env NODE_ENV=development electron ./dist/main/main-bundle.js",
    "prod": "./node_modules/.bin/cross-env NODE_ENV=production electron ./dist",
    "watch": "./node_modules/.bin/webpack --env development -c ./webpack/webpack.config.js -w",
    "build": "./node_modules/.bin/webpack --env production --env sourcemap -c ./webpack/webpack.config.js",
    "build:mac": "./node_modules/.bin/gulp mac",
    "pkg:mac": "./node_modules/.bin/gulp maczip",
    "build:dmg": "./node_modules/.bin/gulp dmg",
    "build:win32": "./node_modules/.bin/gulp win32",
    "pkg:win32": "./node_modules/.bin/gulp win32zip",
    "build:win64": "./node_modules/.bin/gulp win64",
    "pkg:win64": "./node_modules/.bin/gulp win64zip",
    "build:linux32": "./node_modules/.bin/gulp linux32",
    "pkg:linux32": "./node_modules/.bin/gulp linux32zip",
    "build:linux64": "./node_modules/.bin/gulp linux64",
    "pkg:linux64": "./node_modules/.bin/gulp linux64zip",
    "release": "build",
    "test": "./node_modules/.bin/cross-env TZ=UTC jest"
  },
  "build": {
    "appId": "com.qiniu.browser",
    "asar": false,
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "src/static/brand/qiniu.ico"
    }
  },
  "engines": {
    "node": "^14 || ^16"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@types/codemirror": "^5.60.5",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.182",
    "@types/mock-fs": "^4.13.1",
    "@types/react-dom": "^18.0.6",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/request": "^2.48.8",
    "@types/showdown": "^2.0.0",
    "archiver": "^3.0.3",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^6.7.1",
    "electron-builder": "^20.34.0",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^14.1.0",
    "gulp": "^4.0.0",
    "gulp-connect": "^5.7.0",
    "gulp-load-plugins": "^2.0.7",
    "gulp-run": "^1.7.1",
    "html-loader": "^4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.3.1",
    "jshint": "^2.9.6",
    "mini-css-extract-plugin": "^2.6.1",
    "mock-fs": "^5.1.1",
    "npm": "^6.13.4",
    "npm-run-all": "^4.1.3",
    "postcss": "^8.4.16",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.2.3",
    "sass": "^1.54.3",
    "sass-loader": "^13.0.2",
    "source-map-loader": "^3.0.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.3.1",
    "typescript": "^4.8.3",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.9.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@root/walk": "^1.1.0",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.10.2",
    "classnames": "^2.3.1",
    "clipboard": "^2.0.4",
    "codemirror": "^5",
    "crc32-stream": "^4.0.2",
    "csv-stringify": "^5.3.6",
    "downloads-folder": "1.0.3",
    "electron": "18.3.3",
    "events": "^2.0.0",
    "font-awesome": "^4.7.0",
    "form-data": "^4.0.0",
    "js-base64": "^3.4.5",
    "js-md5": "^0.7.3",
    "kodo-s3-adapter-sdk": "0.2.37",
    "lodash": "^4.17.21",
    "mime": "^2.3.1",
    "moment": "^2.22.2",
    "qiniu-path": "^0.0.3",
    "react": "^18.2.0",
    "react-base-table": "^1.13.2",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.0",
    "react-hot-toast": "^2.3.0",
    "react-router-dom": "^6.3.0",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.2",
    "request": "^2.88.0",
    "sanitize-filename": "^1.6.1",
    "semaphore-promise": "^1.2.0",
    "showdown": "^1.9.0"
  }
}
